import stdlib.math;
import person;

def get_point() -> Vec2 {
    let x;
    let y;

    print("Insira a coordenada X: ");
    x = input();
    print("Insira a coordenada Y: ");
    y = input();
    println("");

    return Vec2(float(x), float(y));
}

def main() -> void {
    let p1 = get_point();
    let p2 = get_point();

    print("Distancia entre os pontos: ");
    println(p1.distance(p2));
    println("");
}